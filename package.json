{
  "name": "vant-api",
  "keywords": [
    "vantage",
    "vantjs",
    "vant-environment",
    "Davis Vantage Pro 2",
    "Davis Vantage Vue",
    "weather"
  ],
  "description": "RESTful API for storing and retrieving vantage weather data.",
  "version": "0.6.1",
  "private": false,
  "main": "dist/index.js",
  "author": "Henri Hollmann (harrydehix)",
  "scripts": {
    "start": "node -r source-map-support/register ./dist/service.js",
    "docker-image": "sudo docker build -t vant-api .",
    "build": "npx tsc",
    "docs": "npx typedoc && redocly build-docs api-specification/api.yml --output=docs/specification.html && redocly build-docs api-specification/api.yml --output=public/index.html",
    "watch": "npx tsc --watch",
    "watch-api-specs": "node ./docs-watcher/watch.js",
    "test": "jest"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {}
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/morgan": "^1.9.5",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "destroyable-server": "^1.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "lodash.merge": "^4.6.2",
    "mongoose": "^7.5.0",
    "node-dev": "^8.0.0",
    "pm2": "^5.3.0",
    "pug": "^3.0.2",
    "simple-units": "^1.0.6",
    "string-hash": "^1.1.3",
    "superagent": "^8.1.2",
    "vant-environment": "^2.0.2",
    "vantjs": "^1.0.9",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.5.9",
    "@types/string-hash": "^1.1.1",
    "@types/superagent": "^4.1.18",
    "@types/supertest": "^2.0.13",
    "@types/uuid": "^9.0.3",
    "@types/validator": "^13.11.1",
    "chokidar": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-essentials": "^9.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typed-emitter": "^2.1.0",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2",
    "validator": "^13.11.0"
  }
}
