{
    "name": "vant-api",
    "keywords": [
        "vantage",
        "vantjs",
        "vant-environment",
        "Davis Vantage Pro 2",
        "Davis Vantage Vue",
        "weather"
    ],
    "description": "RESTful API for storing and retrieving vantage weather data.",
    "version": "0.5.0",
    "private": false,
    "main": "dist/index.js",
    "author": "Henri Hollmann (harrydehix)",
    "scripts": {
        "build": "npx tsc",
        "vant-api": "node -r source-map-support/register ./dist/tests/api.js",
        "vant-recorder": "node -r source-map-support/register ./dist/tests/recorder.js",
        "test": "node -r source-map-support/register",
        "docs": "npx typedoc && redocly build-docs api-specification/api.yml --output=docs/specification.html",
        "watch": "npx tsc --watch",
        "service-setup": "sudo bash intialize_vant_api_service.bash intialize_vant_recorder_service.bash && systemctl daemon-reload && systemctl enable vantapi && systemctl enable vantrecorder"
    },
    "exports": {
        ".": "./dist/index.js",
        "./api": "./dist/api/index.js",
        "./recorder": "./dist/recorder/index.js"
    },
    "typesVersions": {
        "*": {
            "api": [
                "dist/api"
            ],
            "recorder": [
                "dist/recorder"
            ]
        }
    },
    "types": "dist/index.d.ts",
    "bin": "dist/tests/test.js",
    "pkg": {
        "scripts": "dist/**/*.js",
        "targets": [
            "node14-linux-x64",
            "node14-macos-x64",
            "node14-win-x64"
        ],
        "outputPath": "bin"
    },
    "dependencies": {
        "@types/morgan": "^1.9.5",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "lodash.merge": "^4.6.2",
        "mongoose": "^7.5.0",
        "node-dev": "^8.0.0",
        "pug": "^3.0.2",
        "simple-units": "^1.0.6",
        "superagent": "^8.1.2",
        "validator": "^13.11.0",
        "vant-environment": "^1.0.6",
        "vantjs": "^1.0.9",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/debug": "^4.1.8",
        "@types/express": "^4.17.17",
        "@types/lodash.merge": "^4.6.7",
        "@types/node": "^20.5.9",
        "@types/superagent": "^4.1.18",
        "@types/uuid": "^9.0.3",
        "@types/validator": "^13.11.1",
        "source-map-support": "^0.5.21",
        "ts-essentials": "^9.4.0",
        "typed-emitter": "^2.1.0",
        "typedoc": "^0.25.1",
        "typescript": "^5.2.2"
    }
}